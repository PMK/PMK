[user]
	name = PMK
	email = webmaster@pmklaassen.com
[color]
	diff = auto
	status = auto
[alias]
	ci = commit
	st = status
	co = checkout
	ba = branch
	lg =  log --graph --pretty=format:"%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset" --abbrev-commit --date=relative
[core]
	excludesfile = "~/.gitexcludes"
